{
  "name": "chessboard",
  "version": "1.0.0",
  "description": "chess board",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm public/app.js",
    "dev": "webpack --mode development",
    "serve": "PORT=8000 serve public",
    "start": "webpack serve --config ./webpack.config.js --mode development",
    "build-dev": "export NODE_ENV=development && webpack",
    "build": "export NODE_ENV=production && webpack --mode production",
    "start-prod": "npm run build && npm run serve",
    "lint": "eslint src --ext .js -c .eslintrc",
    "prettier": "prettier src/**/*.js",
    "lint:fix": "eslint src --ext .js -c .eslintrc --fix",
    "test": "jest --watch --maxWorkers=25%",
    "test:cli": "jest --maxWorkers=50%",
    "precommit": "npm run test:ci && npm run lint"
  },
  "keywords": [
    "chess",
    "react"
  ],
  "author": "Akallabet",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^25.1.3",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-preval": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.6",
    "jest": "^26.6.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "chess-fp": "^0.1.1",
    "clsx": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "serve": "^11.3.2"
  }
}
